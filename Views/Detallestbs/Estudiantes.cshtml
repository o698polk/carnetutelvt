@model carnetutelvt.Models.Detallestb

@{
    ViewData["Title"] = "Details";
}

<div class="hero">
    <h1>Estudiante de la UTELVT</h1>
    <div class="container">

        <form class="form">
            <dl class="row">
                <dt class="col-sm-2">
                    NOMBRES:
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Fullname)
                </dd>
                <dt class="col-sm-2">
                    APELLIDOS:
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Surnames)
                </dd>
                <dt class="col-sm-2">
                    ESPECIALIDAD:
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Specialty)
                </dd>
                <dt class="col-sm-2">
                    FACULTAD:
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Faculty)
                </dd>

            </dl>
        </form>
        <div>
        </div>



